{
  "log": {
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "tag": "vless-tcp-in",
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "e4f8c1a8-1a2b-4c3d-9e5f-123456789abc",
            "level": 0,
            "email": "user@kia-ai.shop"
          }
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": 444,
            "xver": 1
          },
          {
            "path": "/hysteria",
            "dest": 444,
            "xver": 1
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "subs.panel.kia-ai.shop",
          "alpn": [
            "http/1.1"
          ],
          "certificates": [
            {
              "certificateFile": "/root/cert/subs.panel.kia-ai.shop/fullchain.pem",
              "keyFile": "/root/cert/subs.panel.kia-ai.shop/privkey.pem"
            }
          ]
        },
        "tcpSettings": {
          "header": {
            "type": "none"
          }
        }
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "warp",
      "protocol": "wireguard",
      "settings": {
        "mtu": 1420,
        "secretKey": "EPpKdSef2QxJu177KNz/ou1VZ0DHLDJLnRUNIziQYWc=",
        "address": [
          "172.16.0.2/32",
          "2606:4700:110:80e8:4866:aecf:851e:a604/128"
        ],
        "workers": 2,
        "domainStrategy": "ForceIP",
        "reserved": [
          140,
          83,
          41
        ],
        "peers": [
          {
            "publicKey": "bmXOC+F1FxEMF9dyiK2H5/1SUtzH0JuVo51h2wPfgyo=",
            "allowedIPs": [
              "0.0.0.0/0",
              "::/0"
            ],
            "endpoint": "engage.cloudflareclient.com:2408",
            "keepAlive": 0
          }
        ],
        "noKernelTun": false
      }
    }
  ],
  "policy": {
    "levels": {
      "0": {
        "statsUserDownlink": true,
        "statsUserUplink": true
      }
    },
    "system": {
      "statsInboundDownlink": true,
      "statsInboundUplink": true,
      "statsOutboundDownlink": false,
      "statsOutboundUplink": false
    }
  },
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "inboundTag": [
          "vless-tcp-in",
          "hysteria-in"
        ],
        "outboundTag": "direct"
      }
    ]
  },
  "sniffing": {
    "enabled": true,
    "destOverride": [
      "http",
      "tls"
    ],
    "metadataOnly": false
  }
}